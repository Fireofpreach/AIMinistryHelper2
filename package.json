{
  "name": "aiministryhelper2",
  "version": "1.0.0",
  "type": "module",
  "description": "AIMinistryHelper2 project",
  "main": "dist/server/server/index.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"nodemon --watch server --ext ts,json --exec tsx server/index.ts\"",
    "build": "npm run build:client && npm run build:shared && npm run build:server && cpx \"shared/**/*\" dist/shared && cpx \"dist/public/**/*\" dist/server/server/public",
    "build:client": "vite build",
    "build:shared": "tsc -p shared/tsconfig.json",
    "build:server": "tsc -p server/tsconfig.json && tsc-alias -p server/tsconfig.json",
    "clean:server": "rimraf dist/server",
    "start": "node dist/server/server/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.77.1",
    "@types/jsonwebtoken": "^9.0.9",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.511.0",
    "openai": "^4.103.0",
    "prisma": "^5.0.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.3",
    "sqlite3": "^5.1.6",
    "tailwind-merge": "^3.3.0",
    "wouter": "^3.7.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@replit/vite-plugin-cartographer": "^0.2.5",
    "@swc/core": "^1.11.29",
    "@swc/register": "^0.1.10",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/express": "^4.17.21",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.0.1",
    "cpx": "^1.2.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.7",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
